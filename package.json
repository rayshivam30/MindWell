    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^1.9.7",
    "axios": "^1.6.0",
    "expo": "~49.0.15",
    "formik": "^2.4.5",
    "identity-obj-proxy": "^3.0.0",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "~2.12.0",
