  "name": "mental-health-app",
  "version": "1.0.0",
  "description": "Mental Health & Wellness Companion App",
  "main": "index.js",
  "packageManager": "pnpm@8.10.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "backend": "cd backend && pnpm run dev",
